#include <stdio.h>

int main() {
    int n,i;
    scanf("%d", &n);
   int ara[n];
   printf("Enter Numbers: ");
   for(i=0; i<n; i++){
       scanf("%d", &ara[i]);
   }
   int max=ara[1];
   
   for(i=0; i<n; i++){
       printf("%d\n", ara[i]);
       if(ara[i]> max){
           max=ara[i];
       }
   }
   printf("maximum is: %d", max);
   

    return 0;
}
